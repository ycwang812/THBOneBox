<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
	<class name="tw.gov.thb.onebox.model.Record" table="OneBoxRecord" lazy="false">
	    
		<id name="id" column="ID" access="field">
			<generator class="identity" />
		</id>
		
		<property name="query" type="string" column="QUERY" />
		<property name="userName" type="string" column="USER_NAME" />
		<property name="ipAddr" type="string" column="IP_ADDR" />
		<property name="dateTime" type="string" column="DATE_TIME" />
		<property name="authType" type="string" column="AUTH_TYPE" />
		<property name="lang" type="string" column="LANG" />
		<property name="oneBoxName" type="string" column="ONE_BOX_NAME" />
		<property name="apiMaj" type="string" column="API_MAJ" />
		<property name="apiMin" type="string" column="API_MIN" />
		<property name="domain" type="string" column="DOMAIN" />
		<property name="createTime" type="timestamp" column="CREATE_TIME" not-null="true" />
		
	</class>
	
</hibernate-mapping>